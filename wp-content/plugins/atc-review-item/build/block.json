{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/atc-review-item",
  "version": "0.1.0",
  "title": "Atc Review Item",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "attributes": {
    "reviewerName": {
      "type": "string",
      "default": "John Doe"
    },
    "companyName": {
      "type": "string",
      "default": "Company"
    },
    "reviewerImage": {
      "type": "string"
    },
    "numberOfStars": {
      "type": "number",
      "default": 5
    },
    "reviewText": {
      "type": "string",
      "default": "Some Review"
    }
  },
  "textdomain": "atc-review-item",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}